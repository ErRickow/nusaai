# neosantara-ai SDK Environment Variables
# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file to version control

# Security Configuration
# The SDK supports loading security credentials from environment variables
# with the prefix: NEOSANTARA_
#
# Security environment variables:
NEOSANTARA_API_KEY=your_apikey_here

# Debug Configuration
# Enable HTTP request/response logging for debugging
NEOSANTARA_DEBUG=false
